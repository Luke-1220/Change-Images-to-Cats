{
    "manifest_version": 2,
    "name": "Change Images to Cats",
    "description": "Cats",
    "version": "1.0.0.0",
    "description": "ページの画像が、可愛い猫の画像に変わります。",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "run_at": "document_end",
        "all_frames": true
    }],
